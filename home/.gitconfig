[user]
	name = Jason Marcell
	email = jmarcell@apple.com
[github]
    user = jasmarc
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    afrag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    lg = log -p
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    ls = ls-files
[core]
    excludesfile = ~/.gitignore_global
[credential]
    helper = osxkeychain
[push]
    default = matching
[branch]
    autosetuprebase = always
[mergetool]
	keepBackup = false
