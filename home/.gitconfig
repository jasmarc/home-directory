[user]
    name = Jason Marcell
    email = jmarcell@apple.com
[github]
    user = jasmarc
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    afrag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
# [format]
#     pretty = format:%C(yellow)%h %Cblue%>(12)%ad %Cgreen%<(7)%aN%Cred%d %Creset%s
[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    lg = log -p
    lol = log --graph --decorate --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    lola = log --graph --decorate --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --all
    ls = ls-files
[core]
    excludesfile = ~/.gitignore_global
[credential]
    helper = osxkeychain
[push]
    default = upstream
[branch]
    autosetuprebase = always
[merge]
    tool = opendiff
[mergetool]
    keepBackup = false
[rerere]
    enabled = true
[git-up "fetch"]
	prune = true
[sendemail]
	suppresscc = all
#	transferEncoding = base64
    from = jmarcell@apple.com
	transferEncoding = base64
[merge]
	renamelimit=999999